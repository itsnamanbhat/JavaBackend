<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ControllerServlet</servlet-name>
  	<display-name>ControllerServlet</display-name>
  	<description></description>
  	<servlet-class>com.grayMatter.ControllerServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginCServlet</servlet-name>
  	<display-name>LoginCServlet</display-name>
  	<description></description>
  	<servlet-class>com.grayMatter.LoginCServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserSignupServlet</servlet-name>
  	<display-name>UserSignupServlet</display-name>
  	<description></description>
  	<servlet-class>com.grayMatter.UserSignupServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UserLoginServlet</servlet-name>
  	<display-name>UserLoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.grayMatter.UserLoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ControllerServlet</servlet-name>
  	<url-pattern>/ControllerServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginCServlet</servlet-name>
  	<url-pattern>/LoginCServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserSignupServlet</servlet-name>
  	<url-pattern>/UserSignupServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UserLoginServlet</servlet-name>
  	<url-pattern>/UserLoginServlet</url-pattern>
  </servlet-mapping>
</web-app>
